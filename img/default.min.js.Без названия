if(typeof opt.complete=='function'){opt.complete()}};xhr.onerror=function(){alert("Request failed");if(typeof opt.complete=='function'){opt.complete()}};if(typeof opt.start=='function'){opt.start()}
xhr.send(null);return xhr}
