if(!fv){try{axo=new ActiveXObject(_sf);fv=axo.GetVariable(_v)}catch(e){}}
if(fv){fv=fv.split(" ")[1].split(",");fv=fv[0]+"."+fv[1]+" r"+fv[2]}}
return fv?!0:!1}
