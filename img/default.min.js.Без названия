function lnwfile_fullsize(url){if(is_img_lnwfile(url)){url=url.replace(/^http:/,'https:');if(url.match(/\/(_\/[a-z]+\/)?_raw\//)){return url.replace(/\/(_\/[a-z]+\/)?_raw\//,'/').replace(/\/([a-z0-9]+)\/([a-z0-9]+)\/([a-z0-9]+)\.([a-z]+)$/,"/$1$2$3.$4")}else if(url.match(/\/(_\/[a-z]+\/)?_(jpg_(resize|fill|stretch|fit)|fill|stretch|fit|resize)(_images)?\/[0-9]+\/[0-9]+\//)){return url.replace(/\/(_\/[a-z]+\/)?_(jpg_(resize|fill|stretch|fit)|fill|stretch|fit|resize)(_images)?\/[0-9]+\/[0-9]+\//,'/').replace(/\/([a-z0-9]+)\/([a-z0-9]+)\/([a-z0-9]+)\.([a-z]+)$/,"/$1$2$3.$4")}else if(url.indexOf('?')!=-1){return url.substring(0,url.indexOf('?'))}}
return url}
