function lnwajax_response(response,success,error){if(response===null||response===''||typeof response==='undefined'){response={};response.key='CONNECTION_TIMEOUT';response.data='';if(error!=null&&typeof error=='function'){error(response.key,response.data)}else{alert(response.key)}}else if(response.success){if(success!=null&&typeof success=='function'){success(response.data)}}else{if(response.key=='PERMISSION_DENIED'){window.location.href=login_url;return !1}else if(response.key=='PERMISSION_DENIED_CLOSE'){window.location.href=login_url;return !1}
