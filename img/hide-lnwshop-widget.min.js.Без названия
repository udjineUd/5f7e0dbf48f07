var retry_hide_lnwshop_widge_count=0;var retry_hide_lnwshop_widge_limit=10;var hide_lnwshop_widget_class="btn-chat-container";var hideLnwShopWidget=function(){try{document.getElementsByClassName(hide_lnwshop_widget_class)[0].style.display="none";if(document.getElementsByClassName(hide_lnwshop_widget_class)[0].style.display!="none"||retry_hide_lnwshop_widge_count<retry_hide_lnwshop_widge_limit){retry_hide_lnwshop_widge_count++;window.setTimeout(hideLnwShopWidget,500)}}catch(err){console.log(err)}}
try{if(document.readyState!='loading')hideLnwShopWidget();else if(document.addEventListener)document.addEventListener('DOMContentLoaded',hideLnwShopWidget);else document.attachEvent('onreadystatechange',function(){if(document.readyState=='complete')hideLnwShopWidget()})}catch(err){console.log("browser not supported script")}